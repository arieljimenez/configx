[core]
    	autocrlf= false
[push]
    	default = simple
[pull]
    	rebase = true
[rerere]
    	enabled = true
[branch]
    	autosetuprebase = always
[alias]
    	rup = remote update --prune
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "status"]
  added = green
  changed = red bold
  untracked = magenta bold
[color "branch"]
  remote = yellow
